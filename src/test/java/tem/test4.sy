float a[2][3] = {{1,2,3},{4,5,6}};

int main(){

    int m = 1;
    int i = -1;
    int sum = 0;
    while(m){
        i = i + 1;
        if(i == 2){
            break;
        }
        int j = -1;
        while(!!m){
            j = j + 1;
            if(j == 3){
                break;
            }
            sum = sum + a[i][j];
        }

    }
    return sum;
}